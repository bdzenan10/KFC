             x:Class="KFC.History">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="historija" HorizontalOptions="Center" VerticalOptions="Center" Margin="0,100,0,0"></Label>

            <Label x:Name="Bonusi" HorizontalOptions="Center"></Label>
